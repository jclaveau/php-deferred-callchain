{
    "name": "jclaveau/php-deferred-callchain",
    "description": "Register a chain of calls that will be applied on an instance later",
    "license": "MIT",
    "type": "library",
    "keywords": ["asynchronous", "fluent", "chain", "calls", "future", "promise"],
    "authors": [
        {
            "name": "Jean Claveau",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4" : {
            "JClaveau\\Async\\" : "src"
        },
        "files": [
              "src/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/polyfill-php72": "^1.12",
        "jclaveau/php-fluent-trait": "^1.1"
    }
}
