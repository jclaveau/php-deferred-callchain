{
    "name": "jclaveau/php-deferred-callchain",
    "description": "Register a chain of calls that will be applied on an instance later",
    "license": "MIT",
    "type": "library",
    "keywords": ["asynchronous", "fluent", "chain", "calls", "future", "promise", "lazy", "later", "php", "php5", "php7"],
    "authors": [
        {
            "name": "Jean Claveau",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "files": [
            "autoloader.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": ">=5"
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/polyfill-php72": "^1.12",
        "jclaveau/php-fluent-trait": "^1.1",
        "jclaveau/php-visibility-violator": "^1.0"
    }
}
